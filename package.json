{
  "name": "@aivangogh/ph-address",
  "version": "2025.3.2",
  "description": "A collection of philippine geographic data based on PSGC",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "Ivan Gemota",
    "email": "davannarts@gmail.com",
    "url": "https://github.com/aivangogh"
  },
  "repository": {
    "url": "git+ssh://git@github.com/aivangogh/ph-address.git"
  },
  "homepage": "https://github.com/aivangogh/ph-address",
  "license": "MIT",
  "keywords": [
    "philippines",
    "address",
    "ph-address",
    "psgc"
  ],
  "scripts": {
    "build:tsup": "tsup",
    "release": "bun run build && changeset publish",
    "test": "vitest",
    "migrate:psgc": "bun run scripts/migrate-psgc.ts",
    "minify:json": "bun run scripts/minify-json.ts src/data",
    "build": "bun run minify:json && bun run build:tsup",
    "format": "bun run prettier --write ."
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@vitest/coverage-v8": "^2.0.4",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.2",
    "typescript": "^5.2.2",
    "vitest": "^2.0.4",
    "xlsx": "^0.18.5"
  },
  "dependencies": {}
}
